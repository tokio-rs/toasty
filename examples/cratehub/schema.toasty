#[table(user_and_packages)]
model User {
    #[key]
    #[auto]
    id: Id,

    name: String,

    #[unique]
    email: String,

    packages: [Package],
}

#[table(user_and_packages)]
#[key(partition = user_id, local = id)]
model Package {
    #[relation(key = user_id, references = id)]
    user: User,

    user_id: Id<User>,

    #[auto]
    id: Id,

    name: String,
}